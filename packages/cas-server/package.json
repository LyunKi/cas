{
  "name": "cas-server",
  "version": "0.1.0",
  "description": "cas sever",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "postinstall": "pnpm prisma generate",
    "compile": "tsc",
    "start": "pnpm compile && node -r dotenv/config ./dist/index.js",
    "dev": "nodemon --inspect --exec node --experimental-specifier-resolution=node  --loader ts-node/esm -r dotenv/config ./src/index.ts",
    "generate:schema-types": "graphql-codegen-esm",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.0.0",
    "@prisma/client": "^4.5.0",
    "global-agent": "^3.0.0",
    "graphql": "^16.6.0",
    "i18n-js": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.10.14",
    "lodash": "^4.17.21",
    "redis": "^4.3.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cloud-dragon/common-utils": "0.3.0",
    "@cloud-dragon/eslint-config": "0.3.0",
    "@cloud-dragon/prettier-config": "^0.0.3",
    "@cloud-dragon/tsconfig-node": "0.1.0",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-resolvers": "^2.7.5",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "jest-config": "^29.2.1",
    "jest-environment-node": "^29.2.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.5.0",
    "redis-mock": "^0.56.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
